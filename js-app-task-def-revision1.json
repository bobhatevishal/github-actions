{
    "family": "js-app-task-def",
    "networkMode": "awsvpc",
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "1024",
    "memory": "3072",
    "executionRoleArn": "arn:aws:iam::540490923739:role/ecsTaskExecutionRole",
    "taskRoleArn": "arn:aws:iam::540490923739:role/ecsTaskExecutionRole",
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    },
    "containerDefinitions": [
        {
            "name": "my-node-aap",
            "image": "540490923739.dkr.ecr.us-east-2.amazonaws.com/js-app-ecr",
            "essential": true,
            "cpu": 0,
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp",
                    "appProtocol": "http",
                    "name": "defin-port"
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/js-app-task-def",
                    "awslogs-create-group": "true",
                    "awslogs-region": "us-east-2",
                    "awslogs-stream-prefix": "ecs"
                }
            }
        }
    ]
}
